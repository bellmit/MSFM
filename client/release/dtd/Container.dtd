<!ELEMENT containerImpl EMPTY>
<!ATTLIST containerImpl  type  %typeConstraints;  #FIXED 'ManagedProperty'
	mode		   %modeConstraints;  #FIXED 'readWrite'
	propertyType   %propertyTypes; #FIXED 'enum'
	implClassName	 ( com.cboe.infrastructureServices.foundationFramework.BOContainer )  #REQUIRED
	propertyValue CDATA	 #FIXED 'implClassName' > 

<!ELEMENT routeName ( #PCDATA )>
<!ATTLIST routeName  type	  %typeConstraints;  #FIXED 'ManagedProperty'
	mode		  %modeConstraints;  #FIXED 'readWrite'
	propertyType  %propertyTypes;  #FIXED 'string' 
	description CDATA #FIXED 'Not sure about the purpose of this property.' >
